<div class="sidebar col-2" sidebar-group>
    <div class="sidebar-header" user_id="<%= user._id %>" myName="<%= user.fullName%>">
        <a href="<%= home%>/home" class="logo-header">
            <i class="fa fa-snapchat-ghost" aria-hidden="true"></i>
            <span>Snap</span>
        </a>
    </div>
   
        <div class="chat-list">
            <a class="friend-item sidebar-item" href="<%= home%>/chat/chatbot/<%= chatBot._id%>">
                <div class="item" >
                    <i class="fa fa-github-alt" aria-hidden="true"></i>
                    <span>Chat Bot</span>
                </div>
            </a>
            <a class="friend-item sidebar-item" href="<%= home%>/social/friends">
                <div class="item" >
                    <i class="fa fa-user" aria-hidden="true"></i>
                    <span>Friends</span>
                </div>
            </a>
            <a class="friend-item sidebar-item" href="<%= home%>/chat/groupChat">
                <div class="item" >
                    <i class="fa fa-user" aria-hidden="true"></i>
                    <span>Groups</span>
                </div>
            </a>
            <span>Nh√≥m</span>

            <% for(let item of roomGr) { %>
                <a class="friend-item sidebar-item" href="<%= home %>/chat/groupChat/<%= item._id%>">
                    
                    <div class="item" >
                        <div >
                            <img src="<%= item.avatar%>" alt="" class="img-group">
                        </div>
                        <span><%= item.roomName %></span>
                    </div>
                </a>
                <% } %>
        </div>
   
</div>